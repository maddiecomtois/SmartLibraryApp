<?xml version='1.0' encoding='utf-8'?>
<widget version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <access origin="*" />
  
  <feature name="LocationManager">
    <param name="ios-package" value="CDVLocationManager"/>
  </feature>

  <feature name="BLE">
    <param name="ios-package" value="BLECentralPlugin" onload="$IOS_INIT_ON_LOAD"/>
  </feature>

  <feature name="BluetoothLePlugin">
    <param name="ios-package" value="BluetoothLePlugin"/>
  </feature>

  <feature name="Device">
    <param name="ios-package" value="CDVDevice"/>
  </feature>

  <feature name="NfcPlugin">
    <param name="ios-package" value="NfcPlugin"/>
  </feature>

  
</widget>